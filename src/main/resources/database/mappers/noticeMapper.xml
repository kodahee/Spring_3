<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.lalalala.s3.notice">
	<select id="noticeList" parameterType="NoticeDTO" resultType="NoticeDTO">
		SELECT * FROM NOTICE
	</select>
	
	<select id="noticeSelect" parameterType="NoticeDTO" resultType="NoticeDTO">
		SELECT * FROM NOTICE WHERE num = ${num}
	</select>
	
	<insert id="noticeInsert" parameterType="NoticeDTO">
		INSERT INTO notice values(notice_seq.nextval, '${name}', '${title}', '${contents}', sysdate, 0)
	</insert>


</mapper>